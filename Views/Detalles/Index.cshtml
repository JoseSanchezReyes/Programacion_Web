@model IEnumerable<BLOGDeportes.Models.Detalle>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Categoria.NomCategoria)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Evento.NomEvento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Distancia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaDetalle)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Categoria.NomCategoria)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Evento.NomEvento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Distancia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaDetalle)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.DetalleID }) |
            @Html.ActionLink("Details", "Details", new { id=item.DetalleID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.DetalleID })
        </td>
    </tr>
}

</table>
